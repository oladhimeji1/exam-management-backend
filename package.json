{
  "name": "school-exam-management-system",
  "version": "1.0.0",
  "description": "A comprehensive school examination management system backend",
  "main": "dist/app.js",
  "scripts": {
    "start": "node ./src/app.js",
    "dev": "nodemon ./src/app.js",
    "test": "jest",
